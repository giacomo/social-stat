{
  "name": "giacomo/social-stat",
  "description": "Get social stats from Facebook, Twitter and Instagram",
  "require": {
    "facebook/php-sdk-v4": "^5.1",
    "j7mbo/twitter-api-php": "^1.0",
    "mgp25/instagram-php": "dev-master",
    "dirkgroenen/pinterest-api-php": "^0.2.0",
    "vlucas/phpdotenv": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.1",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Giacomo\\SocialStat\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Giacomo\\SocialStat\\Tests\\": "tests"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Giacomo Barbalinardo",
      "email": "info@ready24it.eu"
    }
  ],
  "minimum-stability": "stable"
}
